<?xml version="1.0" encoding="utf-8"?>
<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/changes/1.0.0"
          xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/plugins/maven-changes-plugin/xsd/changes-1.0.0.xsd">
  <properties>
    <title>OpenCms Module generator plugin Project</title>
    <author email="kamomileware@gmail.com">José Ángel García</author>
  </properties>
  <body>
    <release version="1.1" date="2015-02-21" description="Subsequent release">
      <action dev="jagarcia" type="update">
        New parent version 4.
      </action>
      <action dev="jagarcia" type="update">
        Refactor dependencies.
      </action>
    </release>
    <release version="1.0" date="2015-02-1" description="Public release">
      <action dev="jagarcia" type="update">
        Change coordinates of the project to com.kamomileware
        Change parent to maven-plugins-parent.
      </action>
      <action dev="jagarcia" type="update">
        Redesign plugin, taking all cmsshell invocations to opencms-cli-maven-plugin.
        Change maojo to annotations format.
      </action>
      <action dev="jagarcia" type="add">
        Include mojo testing harness skeleton.
      </action>
      <action dev="jagarcia" type="update">
        Includes compatibility with 9.5 opencms version.
      </action>
    </release>
    <release version="0.0.7" date="2012-09-05" description="Nueva Autoria">
      <action dev="joseangel" type="update">
        Empaquetado pasa a com.kw
        Id sigue nueva regla ___-maven-plugin
      </action>
      <action dev="joseangel" type="update">
        Nueva version de plugins y elimina dep con vdos.utils
      </action>
    </release>
    <release version="0.0.6" date="2010-04-20" description="Actualización del proyecto">
      <action dev="joseangel" type="update">
        Implementa semantica de propiedad "attachClasses", que controla
        la presencia de binarios (directorios classes y/o lib) dentro del
        módulo generado. Por defecto es true.
      </action>
      <action dev="joseangel" type="update">
        Elimina la etiqueta por defecto classes para el empaquetado jar
      </action>
      <action dev="joseangel" type="update">
        Elimina la necesidad de pom de proyecto para mojos install-module, uninstall-module, y reload-app
      </action>
      <action dev="joseangel" type="fix">
        Elimina mensaje warning para recurso duplicado en el caso de directorio base del módulo
      </action>
      <action dev="joseangel" type="update">
        Nueva versión del padre
        vdos-desc:1.4
      </action>
      <action dev="joseangel" type="update">
        Documentación de proyecto.
      </action>
      <action dev="joseangel" type="add">
        Enables caché for the module structure
      </action>
    </release>
    <release version="0.0.5" date="2010-02-25" description="Reorganización">
      <action dev="joseangel" type="remove">
        Extrae la funcionalidad de webdavget y svnsync, que queda recogida en
        sendos plugins: maven-webdavget-plugin y maven-svnsync-plugin
      </action>
    </release>
  </body>
</document>
